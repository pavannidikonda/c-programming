// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
void main()
{
   char string[50],revstr[50],i;
   int a;
   printf("Enter a string:");
   scanf("%s",string);
   for(i=strlen(string)-1;i>=0;i--)
   {  
      revstr[strlen(string)-i-1]=string[i]; 
   }
   printf("reversed string:%s\n",revstr);
    a=strcmp(string,revstr);
    if(a==0)
    {
        printf("palindrome");
    }
    else
    {
        printf("not a palindrome");
    }
}
