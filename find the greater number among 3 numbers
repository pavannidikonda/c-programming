#include<stdio.h>
int main()
{
  int a,b,c;
  printf("enter three numbers:");
  scanf("%d%d%d",&a,&b,&c);
  if(a<b)
  {
      if(b<c)
      {
          printf("c is big");
      }
      else
      {
          printf("b is big");
      }
  }else
  {
      if(a<c)
      printf("c is big");
      else
      printf("a is big");
  }
}
