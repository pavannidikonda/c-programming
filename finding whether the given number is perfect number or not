// Online C compiler to run C program online
#include <stdio.h>
int main()
{
    int i,n,sum=0;
    printf("enter the number :");
    scanf("%d",&n);
    for(i=1;i<=n/2;i++)
    {
        if(n%i==0)
        {
            sum=sum+i;
        }
    }
    if(sum==n)
    {
        printf("the given num is perfect number");
    }
    else
    {
        printf("the given num is not perfect number");
    }
    return 0;
}
